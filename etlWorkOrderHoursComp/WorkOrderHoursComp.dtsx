                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select * from
(
SELECT distinct
          QGPL.cd( wlstrx )         AS "OpsDate"        ,
          trim(VARCHAR( WLMCU, 30, 37 ))  AS "WorkCenter"     ,
          IMDSC1                    AS "WorkCenterDesc",
          WLDOCO 					AS "OrderNumber",
          VARCHAR( imlitm, 20, 37 ) AS "PARTNUMBER"     ,
          WLOPSQ * .01				AS "OPSSequence",
          ( imdsc1
                    || '-'
                    || imdsc2 )   AS "PARTDESC"      ,
          VARCHAR( wluom, 3, 37 ) AS "UNITOFMEASURE",
          WLSETL *.01             AS "STANDARDSETUP",
         sum (
                    CASE
                              WHEN wttyr = '2'
                                        THEN wthrw
                                        ELSE 0
                    END
          )
          *.01                                                              AS "ACTUALSETUP"       ,
          DECIMAL(( WLRUNM *.01 ), 15, 2 )                                  AS "STANDARDRUNMACHINE",				--from F3112     
          DECIMAL(( WLRUNL *.01 ), 15, 2 )                                  AS "STANDARDRUNLABOR"  ,				--from F3112
          DECIMAL(( WLMACA *.01 ), 15, 2 )                                  AS "ACTUALRUNMACHINE"  ,				--from F3112
          DECIMAL(( WLLABA *.01 ), 15, 2 )                                  AS "ACTUALRUNLABOR"    ,				--from F3112
          DECIMAL(( WLSOQS / WLUORG ), 15, 2 )                              AS "STANCALCRATIO"     ,				--from F3112

          WLUORG                                                            AS "REQUESTEDQTY"       ,
          WLSOQS                                                            AS "SHIPQTY"            ,
		  DECIMAL((IRRUNL * .01), 15, 2)									AS "PlannedRunLabor"	,				--From f3003
		  DECIMAL((IRRUNM * .01), 15, 2)									As "PlannedRunMachine",				--from f3003
		  DECIMAL (((wlrunm + wlrunl)*.01),15,2)							AS WOHRS,
		  DECIMAL (((wlmaca + wllaba)*.01),15,2)							AS ACTHRS,
		  DECIMAL (((irrunm + irrunl))*.01,15,2)							AS StdHrs,
		  DECIMAL (((irrunm + irrunl)* (wlsoqs/wluorg))*.01,15,2) 			AS StdComHrs
/*

RunMach = irRUMM
RunLab  = irRUNL
Set-Up  = irSETL
WOHRS   = wlRUNM + wlRUNL * .01
ACTHRS  = wlMACA + wlLABA * .01
StdHrs  = RunMach + RunLab * wlUORG * .01
StdComHrs = RunMach + RunLab * wlSOQS * .01


Standard Hours:
Mach Hrs - Sum of St MACHINE	
				St Machine = If "Standard Run Machine" = 0 then 0 else STDCOMHRS
					STDCOMHRS  = "Standard Run Machine" + "Standard Run Labor" x "Stan Calc Ratio" 
Lab Hrs - Sum of St Labor	
				St Machine = If "Standard Run Labor" = 0 then 0 else STDCOMHRS
					STDCOMHRS  = "Standard Run Machine" + "Standard Run Labor" x "Stan Calc Ratio"	
					

Actual Hours:
Mach Hrs - Sum of Ac Machine 
				Ac Machine = If WorkCenter Type = "Machine" then ACTHRS else 0			***Workcenter Type is a hard coded value derived from workcenter ie-either a labor function or machine function
					ACTHRS = "Actual Run Machine" + "Actual Run Labor" 
Lab Hrs - Sum of Ac Labor 
				Ac Machine = If WorkCenter Type = "Labor" then ACTHRS else 0
					ACTHRS = "Actual Run Machine" + "Actual Run Labor"		
Ac Labor WO Hours - If IMSRP4 or IMPRP4 = "LBR" then "Actual Run Machine" + "Actual Run Labor"	
Set-Up Hours - "Actual Set-Up"	
Avg Crew Size - Seamless Average(Crew) by workcenter for the week where crew &gt; 0

Planned Hours:
Mach Hrs - Sum of Pl Machine 
				Pl Machine = If "Planned Run Machine" = 0 then 0 else WOHRS 			***Workcenter Type is a hard coded value derived from workcenter ie-either a labor function or machine function
					WOHRS = "Planned Run Machine" + "Planned Run Labor" 
Lab Hrs - Sum of Pl Labor 
				Pl Machine = If "Planned Run Labor" = 0 then 0 else WOHRS
					WOHRS = "Planned Run Machine" + "Planned Run Labor"		
Pl Labor WO Hours - If IMSRP4 or IMPRP4 = "LBR" then "Plannd Run Machine" + "Planned Run Labor"	
Set-Up Hours - "Actual Set-Up"	
Avg Crew Size - Seamless Average(Crew) by workcenter for the week where crew &gt; 0


*/
					

		  
FROM
          amsdta.f3112 --Shop Floor Routing Instructions
          LEFT JOIN
                    amsdta.F3003 --Join to Routing master to get standards
                    ON
                              --&gt;&gt;&gt;Below joins help in removing dupes
                              irmmcu     = WLMMCU --Join on Location
                              AND irmcu  = WLMCU  --Join on WorkCenter
                              AND iropsq = WLOPSQ --Join on Operations Sequence
                              AND irkit  = WLKIT  --Join on Item Number
          LEFT JOIN
                    amsdta.f31122 --join to work order time transactions
                    ON
                              wtdoco     = wldoco --join on work order
                              AND wtdcto = wldcto --join on doc type
                              AND wtopsq = wlopsq --join on sequence
                              AND wttyr IN( 1,
                                           2 ,
                                           3 ) --Hour types
          LEFT JOIN
                    amsdta.f41002 --Conversion table to get accurate counts for each item
                    ON
                              umitm     = wlkit
                              AND umum  = wluom
                              AND umrum = 'FT'
         LEFT JOIN
                    amsdta.f4101 --join Item master to get meta data for parts
                    ON
                              imitm = wlkit
        LEFT   JOIN
                    amsdta.f4801 --work order master
                    ON
                              wadoco     = wldoco
                              AND wadcto = wldcto
WHERE
	WLSTRX &gt;= 118169 AND WLSTRX &lt;= 118175
          --wldoco = 1025110 
          --        qgpl.cd(wlstrx) &gt;= '2018-06-18'
          --  AND qgpl.cd(wlstrx) &lt;= '2018-06-24'
          AND TRIM( wlmcu ) IN( '59506', --only retrieve data for work centers in legacy report
                               '59502' ,
