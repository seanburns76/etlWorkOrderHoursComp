                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
	DISTINCT qgpl.cd(WLSTRX) AS wlstrx,
	wldoco,
	wldcto,
	Wlmmcu,
	wasrst,
	wlkit,
	wlkitl,
	imdsc1,
	imdsc2,
	imsrp1,
	imprp4,
	imsrp4,
	wlopsq / 100 AS OpSeq,
	Wlopst,
	trim(wlmcu) as wlmcu,
	wldsc1,
	wluorg,
	wlsoqs,
                   irtrt,
	(
		wlrunm + wlrunl
	)*.01 AS WOHRS,
	(
		wlmaca + wllaba
	)*.01 AS ACTHRS,

 irrunm *.01 AS RunMach,
	irrunl *.01 AS RunLab,
	irtimb AS IRTIMB,
	irsetl *.01 AS setup,
	(
		(
			irrunm + irrunl
		)*(
			WLuorg *.01
		)
	)* CASE
		WHEN irtimb = 'U' THEN 1
		WHEN irtimb = '1' THEN.1
		WHEN irtimb = '2' THEN.01
		WHEN irtimb = '3' THEN.001
		WHEN irtimb = '4' THEN.0001
	END AS STDHRS,
	(
		(
			irrunm + irrunl
		)*(
			WLsoqs *.01
		)
	)* CASE
		WHEN irtimb = 'U' THEN 1
		WHEN irtimb = '1' THEN.1
		WHEN irtimb = '2' THEN.01
		WHEN irtimb = '3' THEN.001
		WHEN irtimb = '4' THEN.0001
	END AS StdComHrs,
	imsrp5
FROM
	amsdta.F3112LA
LEFT JOIN amsdta.F30008 ON
	iwmcu = WLMCU
LEFT JOIN amsdta.F3003LA ON
	irkit = WLKIT
	AND iropsq = WLOPSQ
	AND irmcu = WLMCU
	AND WLNXOP = irnxop
	AND irtrt = WLTRT
LEFT JOIN amsdta.f4801 ON
	wadoco = WLDOCO
LEFT JOIN amsdta.F4101 ON
	imitm = WLKIT
WHERE
	WLSTRX &gt;=  ( 
	(substr(year(current date - 6 DAYS),2,1)+1) || 
	substr(year(current date - 6 DAYS),3,2) || 
	(CASE 	WHEN dayofyear(current date - 6 DAYS) &gt; 99 THEN ('' || dayofyear(current date - 6 DAYS))
